{
  "manifest_version": 3,
  "name": "데일리인증",
  "version": "1.0.3",
  "description": "데일리인증을 더 편하게",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://cafe.naver.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://cafe.naver.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
